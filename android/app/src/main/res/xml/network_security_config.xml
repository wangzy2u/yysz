<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!--  默认配置，是否开启Http流量允许-->
    <base-config cleartextTrafficPermitted="true">

        <trust-anchors>
            <certificates
                overridePins="true"
                src="system" />
            <certificates
                overridePins="true"
                src="user" />
        </trust-anchors>
    </base-config>

</network-security-config>